<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <style>
    body {
      padding: 30px;
      font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
      text-align: center;
    }

    #outerColumn {
      display: inline;
      border: black;
    }

    #innerThumb {
      width: 200px;
      height: 300px;
      border: 1px solid #444444;
    }

    #thumbImg {
      max-width: 100%;
      max-height: 100%;
    }

    #innerTitle {
      width: 200px;
      height: 50px;
      border: 1px solid #444444;
      text-align: center;
    }

    #tagBox {
      float: left;
      text-align: left;
    }
  </style>
</head>

<body>
  <div id="tagBox">
    <form id='tableForm' action="/helltaker" method="post">
      <fieldset>
        <legend>TAG</legend>
        <input list="tags" name="tag" id="tag">
        <datalist id="tags">
          <% tagResult.forEach( function(val){ %>
          <option><%=val._desc %></option>
          <% }) %>
        </datalist>
        <input type="submit" value="Submit" />
      </fieldset>
  </div>

  <table id="outerColumn">
    <tr>
      <% var i = 0 %>
      <% results.forEach( function(val){ %>
      <% if ( i > 6 ) { %>
    </tr>
    <tr>
      <% i = 0; } i++;%>
      <td>
        <div id="innerThumb">
          <a href="/helltaker?_index=<%=val._index%>">
            <img id="thumbImg" src="symComicData/<%=val._index%>/000.webp">
          </a>
        </div>
        </form>
        <div id="innerTitle">
          <%=val.title %>
        </div>
      </td>

      <% }) %>
    </tr>
  </table>
</body>

</html>